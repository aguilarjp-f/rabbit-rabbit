//~ Net ROAS Colour
IF [Net ROAS] <0.00 THEN "Loss"
ELSEIF [Net ROAS] >=0.00 AND [Net ROAS] <0.25 THEN "Near Breakeven"
ELSEIF [Net ROAS] >=0.25 AND [Net ROAS] <1.00 THEN "Slight Profit"
ELSEIF [Net ROAS] >=1.00 AND [Net ROAS] <1.50 THEN "Decent Profit"
ELSEIF [Net ROAS] >=1.50 AND [Net ROAS] <2.00 THEN "Strong Profit"
ELSEIF [Net ROAS] >=2.00 AND [Net ROAS] <2.50 THEN "High Profit"
ELSEIF [Net ROAS] >=2.50 THEN "Amazing"
ELSE "Error"
END
